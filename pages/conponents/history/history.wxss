/* pages/conponents/history/history.wxss */
.table{
  width:100%;
  height: 100%;
}
.tr{
  width: 100%;
  height: 50rpx;
  border-bottom: 1px solid cadetblue;
}
.tr:nth-of-type(even){
  background-color: rgb(40, 95, 77);
  color: white;
}
.th,.td{
  float: left;
  width: 50%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}
.th{
  border-bottom: 2px solid cadetblue;
  font-weight: 800;
}