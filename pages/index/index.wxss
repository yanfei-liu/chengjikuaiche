/**index.wxss**/
.v {
  width: 100%;
  height: 100vh;
}

.m {
  width: 100%;
  /* height: 94vh; */
  height: 100%;
}

.maps {
  width: 100%;
  height: 100%;
}

.bottom {
  width: 100%;
  height: 6vh;
}
.bottom view{
  float: left;
  width:30%;
  height: 100%;
  margin: 0;
  text-align: center;
  display: flex;    /*设置显示样式**/
  align-items: center;    /**子view垂直居中*/
  vertical-align: center; /**垂直居中*/
  justify-content: center; /**内容居中*/
  flex-direction:row;
}

.bottom :nth-child(1){
  width:70%;
  background-color: cadetblue;
}

.bottom2{
  margin-left: calc(100vw - 20vw);
  margin-top: calc(100vh - 20vh);
}

.bottom2 view{
  padding: 15rpx;
  background-color: cadetblue;
  border-radius: 50rpx;
  margin-top: 5px;
}
